}

.btn {
  font-size: 25rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 45rpx;
  width: 180rpx;
  margin-bottom: 20rpx;
  margin-top: 20rpx;
  margin-right: 20rpx;
}

.advanceline {
  margin-left: 30rpx;
  margin-right: 30rpx;
  border-top: 1rpx solid #ca9f9f;
}

.tips .add {
  font-size: 20rpx;
}

radio .wx-radio-input {
  width: 30rpx;
  height: 30rpx;
  border: 2rpx solid #999;
  border-radius: 100%;
  background: none;
}

radio .wx-radio-input.wx-radio-input-checked {
  border-color: #ca9f9f;
  background-color: #faf4f2;
}

radio .wx-radio-input.wx-radio-input-checked::before {
  content: '';
  border-radius: 50%;
  background-color: #ca9f9f;
  height: 20rpx;
  width: 20rpx;
}

.rad {
  font-size: 25rpx;
  margin-bottom: 15rpx;
  margin-left: 20rpx;
  margin-top: 5rpx;
}

.radson {
  margin-right: 25rpx;
}

